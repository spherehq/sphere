{
  "name": "@spherehq/cli",
  "description": "Command line tool for managing your sphere content",
  "version": "0.8.0",
  "author": "Jason Brown",
  "homepage": "https://github.com/spherehq/sphere",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p . -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "bin": {
    "sphere": "./bin/run"
  },
  "bugs": "https://github.com/spherehq/sphere/issues/new?labels=packages:cli,bug",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@sindresorhus/slugify": "^0.9.1",
    "@spherehq/database": "^0.6.4",
    "cli-ux": "^5.2.1",
    "email-validator": "^2.0.4",
    "fs-extra": "^7.0.1",
    "inquirer": "^6.3.1",
    "md5-file": "^4.0.0",
    "mdast-util-to-string": "^1.0.5",
    "remark": "^10.0.1",
    "remark-parse": "^6.0.3",
    "remark-rehype": "^4.0.0",
    "remark-retext": "^3.1.2",
    "remark-stringify": "^6.0.4",
    "retext-english": "^3.0.2",
    "terminal-link": "^1.3.0",
    "to-vfile": "^5.0.2",
    "tslib": "^1",
    "unified": "^7.1.0",
    "unist-util-select": "^2.0.0",
    "unist-util-visit": "^1.4.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.22.0",
    "@oclif/tslint": "3.1.1",
    "@types/fs-extra": "5.0.5",
    "@types/inquirer": "6.0.1",
    "@types/md5-file": "4.0.0",
    "@types/node": "10.14.6",
    "globby": "8.0.2",
    "ts-node": "8.1.0",
    "tslint": "5.15.0",
    "typescript": "3.4.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "hooks": {
      "init": "./lib/hooks/init"
    },
    "bin": "sphere",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/spherehq/sphere.git",
  "gitHead": "d406a4185c18d04de90c88ee420d66dad6bd93dc"
}
