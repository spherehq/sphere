{
  "name": "@spherehq/database",
  "description": "Sphere prisma database bindings",
  "version": "0.14.1",
  "author": "Jason Brown",
  "license": "MIT",
  "main": "generated/prisma-client/index.js",
  "scripts": {
    "prepublishOnly": "yarn generate:client && pinst --disable",
    "postpublish": "pinst --enable",
    "postinstall": "yarn generate:prisma-config",
    "deploy": "prisma deploy",
    "start:tunnel:hooks": "ngrok http -subdomain=local.hooks.sphere.sh 8080",
    "start:serverless": "rimraf .build && dotenv -- serverless offline -P=8080",
    "generate:prisma-config": "dotenv -- envsub prisma.yml.dist prisma.yml",
    "generate:client": "prisma generate",
    "postgenerate:client": "tsc --declaration",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "dependencies": {
    "aws-lambda": "^0.1.2",
    "got": "^10.0.1",
    "graphql-cli": "^3.0.11",
    "metascraper": "^5.8.9",
    "metascraper-author": "^5.8.7",
    "metascraper-clearbit": "^5.8.9",
    "metascraper-date": "^5.8.7",
    "metascraper-description": "^5.8.7",
    "metascraper-image": "^5.8.7",
    "metascraper-logo": "^5.8.7",
    "metascraper-publisher": "^5.8.7",
    "metascraper-title": "^5.8.7",
    "metascraper-url": "^5.8.7",
    "node-fetch": "^2.6.0",
    "parse-domain": "^2.3.4",
    "prisma": "^1.30.1",
    "prisma-client-lib": "^1.30.1",
    "serverless": "^1.58.0",
    "snyk": "^1.230.5",
    "url-slug": "^2.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.36",
    "@types/node-fetch": "2.5.4",
    "@types/rimraf": "2.0.3",
    "@types/serverless": "1.18.4",
    "envsub": "4.0.7",
    "ngrok": "3.2.5",
    "pinst": "1.1.1",
    "rimraf": "3.0.0",
    "serverless-offline": "5.12.1",
    "serverless-offline-scheduler": "0.3.8",
    "serverless-plugin-optimize": "4.1.1-rc.1",
    "serverless-plugin-typescript": "1.1.9"
  },
  "gitHead": "3f7a82d4c57b68986a27a3b3bc33cc52ed9ca713",
  "snyk": true
}
